{
  "manifest_version": 3,
  "name": "ChatGPT One-Click Delete",
  "version": "1.0",
  "description": "Skip confirmation dialog when deleting ChatGPT conversations",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://chat.openai.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": []
}
